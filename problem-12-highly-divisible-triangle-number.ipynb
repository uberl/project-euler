   "source": [
    "from sympy import divisors\n",
    "\n",
    "\n",
    "def number_of_divisors(n: int) -> int:\n",
    "    return len(divisors(n))\n",
    "\n",
    "\n",
    "triangle_number = 1\n",
    "iterator = 1\n",
    "\n",
    "while number_of_divisors(triangle_number) < 500:\n",
    "    iterator += 1\n",
    "    triangle_number += iterator\n",
    "\n",
    "triangle_number"
   ]
  }
